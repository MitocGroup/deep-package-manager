  "main": "lib.compiled/bootstrap.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MitocGroup/deep-package-manager.git"
  },
  "scripts": {
    "test": "babel-node `which isparta` cover --include 'lib/**/*.js' _mocha -- 'test/**/*.spec.js' --reporter spec --ui tdd --recursive",
    "compile": "if [ -d 'lib/' ]; then BABEL_ENV=production babel lib/ --out-dir lib.compiled/; fi",
    "obfuscate-compiled": "if [ -d 'lib/' ]; then npm list -g --depth 0 uglify > /dev/null 2>&1 || npm install uglify -g; for f in $(find lib.compiled -type f -name *.js); do uglify -s ${f} -o ${f}; done; fi",
    "prepublish": "npm run compile && npm run obfuscate-compiled"
  },
  "dependencies": {
    "deep-db": "^1.x.x",
    "deep-core": "^1.x.x",
    "joi": "^6.4.x",
    "jsonfile": "^1.1.x",
    "underscore.string": "^2.3.x",
    "moment": "^2.10.x",
    "archiver": "^0.14.x",
    "sync-exec": "^0.6.x",
    "mkdirp": "^0.5.x",
    "mime": "^1.3.x",
    "crc": "^3.3.x",
    "tar": "^2.1.x",
    "fstream": "^1.0.x",
    "fs-extra": "^0.23.x",
    "tmp": "^0.0.x",
    "crypto": "^0.0.x",
    "ignore": "^2.2.x",
    "raml-validate": "^1.0.x",
    "raml-sanitize": "^1.0.x",
    "object-merge": "^2.5.x",
    "graceful-fs": "^4.1.x",
    "find-assets": "^0.3.x",
    "css-find-assets": "MitocGroup/css-find-assets",
    "css-parse": "^2.0.x",
    "readline-sync": "^1.2.x"
  },
  "devDependencies": {
    "aws-sdk": "^2.2.x",
    "chai": "^3.2.x",
    "sinon": "^1.17.x",
    "sinon-chai": "^2.8.x"
  },
  "engines": {
    "node": ">=0.12 <5.0.0",
    "npm": ">=2.10"
  },
  "os": [
    "!win32"
  ],
  "preferGlobal": false,
  "analyze": true
}
